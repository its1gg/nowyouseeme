{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "description": "A simple Chrome extension with buttons to run JavaScript code.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
      "<all_urls>"
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  